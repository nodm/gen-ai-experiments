{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["es2022", "dom", "ES2022.String"],
    "module": "NodeNext",
    "outDir": "build",
    "rootDir": "./src",
    "target": "ES2020"
  },
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
